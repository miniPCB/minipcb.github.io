{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "miniPCB Catalog",
      "type": "python",
      "request": "launch",
      "module": "minipcb_catalog.main",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": [
        "--root", "${workspaceFolder}"
      ],
      "env": {
        "PYTHONUNBUFFERED": "1"
      }
    }
  ]
}
